<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">

	<servlet>
		<servlet-name>Homepage</servlet-name>
		<servlet-class>com.umi.oztees.servlets.HomepageServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Homepage</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Products</servlet-name>
		<servlet-class>com.umi.oztees.servlets.ProductsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Products</servlet-name>
		<url-pattern>/products/*</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Csv</servlet-name>
		<servlet-class>com.umi.oztees.servlets.CSVServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Csv</servlet-name>
		<url-pattern>/csv</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Order</servlet-name>
		<servlet-class>com.umi.oztees.servlets.OrderServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Order</servlet-name>
		<url-pattern>/order</url-pattern>
	</servlet-mapping>
</web-app>
